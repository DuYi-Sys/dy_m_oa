<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.duyi.oa.dao.QuestionDao">
<insert id="addQuestion" useGeneratedKeys="true" keyProperty="id" parameterType="map">
         insert into questitopic_idon_bank(upload_id,topic_id,title,question_body, question_analysis, pic,status)
         values(#{upload_id},#{},#{title},#{question_body},#{question_analysis}, #{pic}, #{status})
    </insert>
</mapper>